{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "magadfsea00001"
		},
		"store_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'store'"
		},
		"blobsinkuser04_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"blobsinkuser04_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "openhackuser04"
		},
		"blobsource_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"blobsource_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "filesopenhack"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"store_connectionString": {
						"value": "[parameters('store_connectionString')]"
					},
					"blobsinkuser04_properties_typeProperties_fileName": {
						"value": "[parameters('blobsinkuser04_properties_typeProperties_fileName')]"
					},
					"blobsinkuser04_properties_typeProperties_folderPath": {
						"value": "[parameters('blobsinkuser04_properties_typeProperties_folderPath')]"
					},
					"blobsource_properties_typeProperties_fileName": {
						"value": "[parameters('blobsource_properties_typeProperties_fileName')]"
					},
					"blobsource_properties_typeProperties_folderPath": {
						"value": "[parameters('blobsource_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}